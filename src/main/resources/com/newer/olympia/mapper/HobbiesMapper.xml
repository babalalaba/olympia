    <select id="FindUtableall" resultMap="Table_t" parameterType="int">
       SELECT t.*,u.User_id,u.User_name,b.*,p.Picture_path AS User_img FROM user_table t,USER u,blogs b,picture p WHERE t.User_id=u.User_id
       AND t.User_id=b.User_id AND u.User_img=p.Picture_id AND t.User_id=#{User_id} AND t.Blogs_id=#{Blogs_id}
