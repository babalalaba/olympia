    <select id="findAllByUser" resultMap="Apply_a" parameterType="int">
       select a.*,u.User_name,p.Picture_path as User_img from apply a,user u,picture p  where
        u.User_img=p.Picture_id and a.User_id=u.User_id and a.user_id = #{User_id}
    </select>
